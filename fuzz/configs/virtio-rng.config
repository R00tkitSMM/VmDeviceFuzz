module="virtio-rng.ko"
harness="harness_blk_char.so"
n_request_irqs=0
loglevel=8
devtype=1
nqueues=4
interface="eth0"
vid=0x0
did=0x4
#Note: enbale to trigger bug
fuzz_dma=0
apply_hacks=1
minimize_delay=1
minimize_timeout=1
minimize_timebefore=1
minimize_timeafter=1
minimize_wq_delay=1
target_irq=1
use_done_cb=1
vio_nofuzz=[0x010, 0x000, 0x034, 0x070, 0x060, 0x44]
features_set_mask_low=0x0
features_set_mask_high=0x1
