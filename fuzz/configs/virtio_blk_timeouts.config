module="virtio_blk.ko"
loglevel=8
#harness="fuzz_disk.so"
harness="fuzz_virtio.so"
request_irqs=0xffffff
devtype=1
nqueues=4
interface="eth0"
vid=0x0
did=0x2
fuzz_dma=0
extra_io=1
drain_irqs=1
minimize_delay=1
minimize_timeout=1
minimize_timebefore=1
minimize_timeafter=1
minimize_wq_delay=1
features_set_mask_low=0x20000
features_set_mask_high=0x1
features_unset_mask_low=0x0
features_unset_mask_high=0x4
moddeps=["virtio.ko", "virtio_ring.ko", "virtio_mmio.ko"]
